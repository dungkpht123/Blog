{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch MyBlogApplication",
      "request": "launch",
      "mainClass": "com.site.blog.my.core.MyBlogApplication",
      "projectName": "my-blog"
    },
    {
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app"
      ],
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "d:/NAM3/javaweb/My-Blog",
      "program": "d:/NAM3/javaweb/My-Blog/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}